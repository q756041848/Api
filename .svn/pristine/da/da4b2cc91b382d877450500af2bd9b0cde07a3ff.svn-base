<script type="text/javascript">
    function recover(name) {
        layer.confirm('确认要导入数据吗？', {icon: 0}, function (index) {
            layer.close(index);
            $.post("<?php echo url('Sys/restoreData'); ?>",{sqlfilepre:name},function(data){
                if(data.status==1){
                    layer.alert(data.msg, {icon: 6}, function(index){
                        layer.close(index);
                        window.location.href=data.url;
                    });
                }else{
                    layer.alert(data.msg, {icon: 2});
                }
            });
            layer.close(index);
        });
    }
    function del(name) {
        layer.confirm('确认要删除该备份文件吗？', {icon: 3}, function (index) {
            $.post('<?php echo url("delSqlFiles"); ?>',{sqlfilename: name}, function (data) {
                if (data.status == 1) {
                    layer.alert(data.msg, {icon: 6}, function(index){
                        layer.close(index);
                        window.location.href=data.url;
                    });
                }else{
                    layer.msg(data.info,{icon:5});
                }
            });
            layer.close(index);
        });
    }
</script>
