@keyframes hover-color {
    from {
        border-color: silver
    }
    to {
        border-color: #0099fc
    }
}

.sc-radio,
.sc-checkbox {
    line-height: 24px;
    display: inline-block;
    margin: 0
}

.sc-radio input,
.sc-checkbox input {
    position: absolute;
    display: none
}

.sc-radio input[disabled],
.sc-checkbox input[disabled] {
    cursor: not-allowed
}

.sc-radio input+.inSpan,
.sc-checkbox input+.inSpan {
    position: relative;
    display: block;
    cursor: pointer;
    vertical-align: middle;
    height: 13px;
    line-height: 13px;
    padding-left: 22px
}

.sc-radio input+.inSpan {
    height: 14px;
    line-height: 14px
}

.sc-radio input+.inSpan:hover:before,
.sc-checkbox input+.inSpan:hover:before {
    animation-duration: .4s;
    animation-fill-mode: both;
    animation-name: hover-color
}

.sc-radio input+.inSpan:before,
.sc-checkbox input+.inSpan:before {
    position: absolute;
    top: 0;
    left: 0;
    display: inline-block;
    width: 12px;
    height: 12px;
    content: '';
    border: 1px solid silver
}

.sc-radio input+.inSpan:after,
.sc-checkbox input+.inSpan:after {
    position: absolute;
    display: none;
    content: ''
}

.sc-radio input[disabled]+.inSpan,
.sc-checkbox input[disabled]+.inSpan {
    cursor: not-allowed;
    color: #e4e4e4
}

.sc-radio input[disabled]+.inSpan:hover,
.sc-radio input[disabled]+.inSpan:before,
.sc-radio input[disabled]+.inSpan:after,
.sc-checkbox input[disabled]+.inSpan:hover,
.sc-checkbox input[disabled]+.inSpan:before,
.sc-checkbox input[disabled]+.inSpan:after {
    cursor: not-allowed
}

.sc-radio input[disabled]+.inSpan:hover:before,
.sc-checkbox input[disabled]+.inSpan:hover:before {
    border: 1px solid #e4e4e4;
    animation-name: none
}

.sc-radio input[disabled]+.inSpan:before,
.sc-checkbox input[disabled]+.inSpan:before {
    border-color: #e4e4e4
}

.sc-radio input:checked+.inSpan:before,
.sc-checkbox input:checked+.inSpan:before {
    animation-name: none
}

.sc-radio input:checked+.inSpan:after,
.sc-checkbox input:checked+.inSpan:after {
    display: block
}

.sc-radio input+.inSpan:before {
    border-radius: 50%
}

.sc-radio input+.inSpan:after {
    top: 4px;
    left: 4px;
    width: 6px;
    height: 6px;
    border-radius: 50%;
    background: #0099fc
}

.sc-radio input:checked+.inSpan:before {
    border: 1px solid #0099fc
}

.sc-radio input:checked[disabled]+.inSpan:before {
    border: 1px solid #c9e2f9
}

.sc-radio input:checked[disabled]+.inSpan:after {
    background: #c9e2f9
}

.sc-checkbox input+.inSpan:before {
    border-radius: 2px
}

.sc-checkbox input+.inSpan:after {
    top: 1px;
    left: 5px;
    box-sizing: border-box;
    width: 5px;
    height: 10px;
    transform: rotate(45deg);
    border-width: 2px;
    border-style: solid;
    border-color: #0099fc;
    border-top: 0;
    border-left: 0
}

.sc-checkbox input:checked+.inSpan:before {
    border: 1px solid #0099fc;
    background: #fff
}

.sc-checkbox input:checked[disabled]+.inSpan:before {
    border: #c9e2f9;
    background: #c9e2f9
}

